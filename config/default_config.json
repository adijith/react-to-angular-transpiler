{
  "parser": {
    "type": "babel",
    "options": {
      "plugins": ["jsx"],
      "sourceType": "module"
    }
  },
  "transformer": {
    "strictMode": false,
    "preserveComments": true
  },
  "generator": {
    "indentSize": 2,
    "useTabs": false,
    "lineEndings": "lf",
    "includeSpecs": true,
    "includeModule": false
  },
  "output": {
    "directory": "output",
    "createModule": false,
    "createSpec": true
  }
}

